.toggle-menu {
    display: none;
    position: relative;
    width: 3.5em;
    height: 3.5em;
    padding: .75em;
    transform: rotate(0);
    transition: .5s ease-in-out;
    cursor: pointer;
    background: #242935;
    z-index: 11
}

@media (max-width: 767px) {
    .toggle-menu {
        position: absolute;
        left: -1.5em
    }
}

.toggle-menu a {
    position: relative;
    display: block;
    width: 2em;
    height: 2em
}

.toggle-menu a span {
    display: block;
    position: absolute;
    height: .125em;
    width: 100%;
    background: #fff;
    opacity: 1;
    right: 0;
    transform: rotate(0);
    transition: .25s ease-in-out
}

.toggle-menu a span:nth-child(1) {
    top: .375em
}

.toggle-menu a span:nth-child(2), .toggle-menu a span:nth-child(3) {
    top: 1em
}

.toggle-menu a span:nth-child(4) {
    top: 1.625em;
    width: 70%
}

.toggle-menu a:hover span:nth-child(4) {
    width: 100%
}

.toggle-menu.open a span {
    background: #fff
}

.toggle-menu.open a span:nth-child(4) {
    width: 0%
}

.toggle-menu.open a:hover span:nth-child(4) {
    width: 0%
}

.toggle-menu.open span:nth-child(1) {
    top: 1em;
    width: 0%;
    right: 50%
}

.toggle-menu.open span:nth-child(2) {
    transform: rotate(-45deg)
}

.toggle-menu.open span:nth-child(3) {
    transform: rotate(45deg)
}

.toggle-menu.open span:nth-child(4) {
    top: 1em;
    width: 0;
    right: 50%
}

@media (max-width: 992px) {
    .toggle-menu {
        display: block
    }
}

button {
    -webkit-appearance: button;
    width: auto;
    border: 0;
    box-shadow: none;
    background: 0 0;
    outline: 0;
    background: 100% 0;
    border-radius: 0;
    cursor: pointer
}

button:focus {
    outline: 0
}

.anchor {
    transition: color .3s;
    position: relative;
    display: inline-block;
    color: #fff;
    border-radius: 3.55em;
    padding: .673em 1.5em;
    text-align: center;
    font-size: 1.125em;
    border: none;
    min-width: 16.816em;
    -webkit-backface-visibility: hidden;
    -moz-osx-font-smoothing: grayscale;
    box-shadow: 0 0 15px rgba(0, 0, 0, .1);
    z-index: 1;
    background: linear-gradient(to left, #001f3d 0, #9b622a 100%);
    overflow: hidden
}

.anchor:after, .anchor:before {
    content: '';
    position: absolute;
    height: 100%;
    width: 100%;
    bottom: 100%;
    right: 0;
    z-index: -1;
    transition: transform .3s;
    transition-timing-function: cubic-bezier(.75, 0, .125, 1)
}

.anchor:before {
    background: #001f3d
}

.anchor:after {
    background: #9b622a
}

.anchor:hover {
    color: #fff;
    text-decoration: none
}

.anchor:hover:after, .anchor:hover:before {
    transform: translate3d(0, 100%, 0)
}

.anchor:hover:after {
    transition-delay: 175ms
}

.white--bgback:after {
    border-color: #001f3d
}

.white--bgback:hover {
    color: #001f3d
}

.white--bgback:hover .arrow-icon {
    background: url(arrow-brown.svg) no-repeat
}

.with--arrow {
    display: inline-flex;
    align-items: center;
    justify-content: center
}

.with--arrow .arrow-icon {
    display: inline-block;
    width: .875em;
    height: .6875em;
    background-size: contain;
    margin-right: 1em;
    background: url(arrow.svg) no-repeat;
    transform: rotateY(180deg)
}

.with--arrow .log-icon {
    display: inline-block;
    width: .875em;
    height: 1.0625em;
    background-size: contain;
    margin-left: .75em;
    background: url(login.svg) no-repeat
}

.outline--anchor {
    background: 0 0;
    color: #fff;
    padding: .625em 1.375em
}

.outline--anchor:before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
    background: 0 0;
    border: 1px solid #001f3d;
    transition: all .3s
}

.outline--anchor:after {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
    opacity: 0;
    border: 0;
    background: linear-gradient(to left, #001f3d 0, #9b622a 100%);
    transition: all .3s;
    transform: scale(.5, .5)
}

.outline--anchor:hover {
    color: #fff
}

.outline--anchor:hover:before {
    opacity: 0;
    transform: scale(1.2, 1.2)
}

.outline--anchor:hover:after {
    opacity: 1;
    transform: scale(1, 1)
}

