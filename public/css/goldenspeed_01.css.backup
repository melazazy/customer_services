}

.file-upload .file-select input[type=file] {
    z-index: 100;
    cursor: pointer;
    position: absolute;
    height: 100%;
    width: 100%;
    top: 0;
    right: 0;
    opacity: 0
}

.file-upload .file-select.file-select-disabled {
    opacity: .65
}

.file-upload .file-select.file-select-disabled:hover {
    cursor: default;
    display: block;
    border: 2px solid #dce4ec;
    color: #34495e;
    cursor: pointer;
    height: 40px;
    line-height: 40px;
    margin-top: 5px;
    text-align: right;
    background: #fff;
    overflow: hidden;
    position: relative
}

.file-upload .file-select.file-select-disabled:hover .file-select-button {
    background: #dce4ec;
    color: #666;
    padding: 0 10px;
    display: inline-block;
    height: 40px;
    line-height: 40px
}

.file-upload .file-select.file-select-disabled:hover .file-select-name {
    line-height: 40px;
    display: inline-block;
    padding: 0 10px
}

.section--adds {
    position: relative;
    background-size: cover;
    height: 59.375em;
    width: 100%;
    display: flex;
    flex-flow: column;
    justify-content: center;
    overflow: hidden;
    margin-top: 5em
}

@media (max-width: 1400px) {
    .section--adds {
        margin-top: 15em
    }
}

@media (max-width: 1100px) {
    .section--adds {
        height: auto
    }
}

@media (max-width: 767px) {
    .section--adds {
        background: #000 !important
    }
}

.section--adds h2 {
    color: #fff
}

.section--adds p {
    color: #fff
}

.section--adds:after, .section--adds:before {
    position: absolute;
    top: auto;
    right: 0;
    width: 10em;
    height: 12.612em;
    background: url(arrowback-white.svg);
    background-size: 1.125em 1.125em;
    z-index: 0;
    opacity: .5;
    bottom: -.3em
}

@media (min-width: 1025px) {
    .section--adds:after, .section--adds:before {
        content: ''
    }
}

.section--adds:before {
    top: auto;
    height: 34em;
    background: url(white-dots.svg);
    background-size: 1.5em 1.5em;
    bottom: 12.5em
}

.section--adds .addside__text {
    position: absolute;
    bottom: 1.5em;
    left: 0;
    z-index: 1
}

.section--adds .addside__text p {
    margin: 0;
    font-size: 7em;
    font-weight: 600;
    color: #fff;
    opacity: .1;
    line-height: .7
}

@media (max-width: 1100px) {
    .section--adds .addside__text p {
        font-size: 4em
    }
}

@media (max-width: 767px) {
    .section--adds .addside__text p {
        font-size: 2.5em
    }
}

.section--adds .inner-container {
    position: relative;
    z-index: 1
}

.section--adds .new__overlay {
    position: absolute;
    top: 0;
    right: 0;
    height: 100%;
    width: 100%;
    background: #000;
    opacity: .4
}

.advertise__wrap {
    width: 49%
}

@media (max-width: 767px) {
    .advertise__wrap {
        width: 100%
    }
}
